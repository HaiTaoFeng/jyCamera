<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pwithe.jycamera">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.hardware.camera" />
    <uses-permission android:name="android.hardware.camera.autofocus" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <!-- 录音的权限 -->
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <application
        android:name="com.pwithe.jycamera.CommApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="jyCamera"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.pwithe.jycamera.CameraActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.pwithe.jycamera.MainActivity">
        </activity>

    </application>

</manifest>